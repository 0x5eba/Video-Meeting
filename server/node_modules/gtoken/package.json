    "mime": "^2.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.7",
    "@types/jws": "^3.1.0",
    "@types/mime": "^2.0.0",
    "@types/mocha": "^5.0.0",
    "@types/nock": "^10.0.0",
    "@types/node": "^10.0.3",
    "codecov": "^3.0.2",
    "gts": "^1.0.0",
    "intelli-espower-loader": "^1.0.1",
    "js-green-licenses": "^1.0.0",
